[include]
    path = ~/.my_gitconfig
[core]
	excludesfile = ~/.gitignore_global
	editor = /usr/local/bin/vim
	pager = /usr/local/Cellar/git/$(ls -1 /usr/local/Cellar/git/ | sort -dnr | head -1)/share/git-core/contrib/diff-highlight/diff-highlight | less
[alias]
  cof = !git for-each-ref --format='%(refname:short)' refs/heads | fzf --preview='git log --oneline --color=always --first-parent --no-merges {}'| xargs git checkout
